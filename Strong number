#include<stdio.h>

int main()

{

    int num,r,i,fact,temp,sum=0;

    printf("enter the number\n");

    scanf("%d",&num);

    temp=num;

    while(num)

    {

        i=1,fact=1;

    r=num%10;

    while(i<=r)

    {

        fact=fact*i;

        i++;

    }

    sum=sum+fact;

    num=num/10;

    }

    if(sum==temp)

    {

        printf("given number is strong number");

    }

    else

    {

        printf("given number is not a strong number");

    }

}
