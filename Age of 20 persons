#include<stdio.h>

void main()

{

    int num,age,i;

    printf("enter the number\n");

    scanf("%d",&num);

    for(i=1;i<=num;i++)

    {

        printf("enter the age\n");

        scanf("%d",&age);

        if(age<=4)

        {

            printf("baby age\n");

        }

        else if(age<=19)

        {

            printf("middle age\n");

        }

        else

        {

            printf("adult age\n");

        }

    }

}
